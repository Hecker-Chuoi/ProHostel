<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.displayRoomListAsTableController">
   <children>
      <ScrollPane prefHeight="650.0" prefWidth="860.0" AnchorPane.topAnchor="0.0">
         <content>
            <TableView fx:id="tableView" prefHeight="648.0" prefWidth="858.0">
              <columns>
                <TableColumn fx:id="idColumn" prefWidth="100.0" text="Mã phòng" />
                <TableColumn fx:id="floorColumn" prefWidth="75.0" text="Tầng" />
                  <TableColumn fx:id="numberColumn" prefWidth="75.0" text="Số phòng" />
                  <TableColumn fx:id="roomTypeColumn" prefWidth="125.0" text="Loại phòng" />
                  <TableColumn fx:id="statusColumn" prefWidth="125.0" text="Trạng thái" />
                  <TableColumn fx:id="customerColumn" prefWidth="150.0" text="Đại diện khách hàng" />
                  <TableColumn fx:id="numOfCustomers" text="Số người ở" />
                  <TableColumn fx:id="fixColumn" prefWidth="62.0" text="Sửa" />
                  <TableColumn fx:id="delColumn" prefWidth="62.0" text="Xóa" />
              </columns>
            </TableView>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
